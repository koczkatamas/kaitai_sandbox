{
  "meta": {
    "id": "pcap",
    "file-extension": "pcapdump",
    "endian": "le"
  },
  "seq": [
    {
      "id": "header",
      "type": "header"
    },
    {
      "id": "packets",
      "type": "packet",
      "repeat": "eos"
    }
  ],
  "types": {
    "header": {
      "seq": [
        {
          "id": "magic_number",
          "contents": [
            212,
            195,
            178,
            161
          ]
        },
        {
          "id": "version_major",
          "type": "u2"
        },
        {
          "id": "version_minor",
          "type": "u2"
        },
        {
          "id": "thiszone",
          "type": "s4"
        },
        {
          "id": "sigfigs",
          "type": "u4"
        },
        {
          "id": "snaplen",
          "type": "u4"
        },
        {
          "id": "network",
          "type": "u4",
          "enum": "linktype"
        }
      ]
    },
    "packet": {
      "seq": [
        {
          "id": "ts_sec",
          "type": "u4"
        },
        {
          "id": "ts_usec",
          "type": "u4"
        },
        {
          "id": "incl_len",
          "type": "u4"
        },
        {
          "id": "orig_len",
          "type": "u4"
        },
        {
          "id": "body",
          "size": "incl_len",
          "if": "_root.header.network != linktype::ppi and _root.header.network != linktype::ethernet"
        },
        {
          "id": "ppi_body",
          "type": "packet_ppi",
          "size": "incl_len",
          "if": "_root.header.network == linktype::ppi"
        },
        {
          "id": "ethernet_body",
          "type": "ethernet_frame",
          "size": "incl_len",
          "if": "_root.header.network == linktype::ethernet"
        }
      ]
    },
    "packet_ppi": {
      "seq": [
        {
          "id": "header",
          "type": "packet_ppi_header"
        },
        {
          "id": "fields",
          "type": "packet_ppi_field",
          "repeat": "eos"
        }
      ]
    },
    "packet_ppi_header": {
      "seq": [
        {
          "id": "pph_version",
          "type": "u1"
        },
        {
          "id": "pph_flags",
          "type": "u1"
        },
        {
          "id": "pph_len",
          "type": "u2"
        },
        {
          "id": "pph_dlt",
          "type": "u4"
        }
      ]
    },
    "packet_ppi_field": {
      "seq": [
        {
          "id": "pfh_type",
          "type": "u2"
        },
        {
          "id": "pfh_datalen",
          "type": "u2"
        },
        {
          "id": "body",
          "size": "pfh_datalen"
        }
      ]
    },
    "radio_802_11_common_body": {
      "seq": [
        {
          "id": "tsf_timer",
          "type": "u8"
        },
        {
          "id": "flags",
          "type": "u2"
        },
        {
          "id": "rate",
          "type": "u2"
        },
        {
          "id": "channel_freq",
          "type": "u2"
        },
        {
          "id": "channel_flags",
          "type": "u2"
        },
        {
          "id": "fhss_hopset",
          "type": "u1"
        },
        {
          "id": "fhss_pattern",
          "type": "u1"
        },
        {
          "id": "dbm_antsignal",
          "type": "s1"
        },
        {
          "id": "dbm_antnoise",
          "type": "s1"
        }
      ]
    }
  },
